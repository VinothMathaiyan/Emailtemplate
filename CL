  --duet-color-border: #272727;

  --duet-font: 'Barclays Effra', Helvetica, Arial, sans-serif;
  --duet-font-normal: 400;
  --duet-font-bold: 600;

  --duet-radius: 4px;
  --duet-z-index: 600;
}

