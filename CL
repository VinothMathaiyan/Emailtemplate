<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script>
  Qualtrics.SurveyEngine.addOnload(function () {
    // Add hover effect for each option box
    $('.ChoiceStructure').hover(
      function () {
        // On hover: Change hyperlink color to white
        $(this).find('a').css('color', '#FFFFFF');
      },
      function () {
        // On mouse out: Revert hyperlink color to original
        $(this).find('a').css('color', '#1A73E8');
      }
    );
  });
</script>
