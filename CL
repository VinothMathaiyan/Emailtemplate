import pandas as pd

# Load the Excel file
file_path = 'your_file.xlsx'
df = pd.read_excel(file_path)

# List of values to exclude
exclude_values = [
    '${e://Field/pbimg}%',
    '${e://Field/RC}',
    'click to write the question text'
]

# Filter out rows where Column B contains only the specified values
df_filtered = df[~df['B'].isin(exclude_values)]

# Save the filtered DataFrame back to an Excel file
output_file_path = 'filtered_file.xlsx'
df_filtered.to_excel(output_file_path, index=False)

print(f"Filtered file saved to {output_file_path}")
