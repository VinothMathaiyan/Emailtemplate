<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.8.2/angular.min.js"></script>  <link rel="stylesheet" href="https://netdna.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.8.2/angular-ui-bootstrap.min.js"></script>

<div ng-app="myApp" ng-controller="MyCtrl">
  <div class="well well-sm">
    <p>Selected Date: {{ selectedDate }}</p>
    <uib-datepicker ng-model="selectedDate" date-format="yyyy-MM-dd"></uib-datepicker>
  </div>
</div>

<script>
var app = angular.module('myApp', ['ui.bootstrap']);
app.controller('MyCtrl', function($scope) {
  $scope.selectedDate = new Date(); // Initial selected date
});
</script>
