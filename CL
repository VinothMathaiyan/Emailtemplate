Qualtrics.SurveyEngine.addOnload(function() {
  // Create the Duet Date Picker element
  var duetDatePicker = document.createElement("duet-date-picker");
  duetDatePicker.setAttribute("first-day-of-week", "0");
  duetDatePicker.setAttribute("identifier", "date");
  
  // Append the date picker to the container
  document.getElementById("duet-date-picker-container").appendChild(duetDatePicker);

  // Listen for when a date is selected and log it to the console
  duetDatePicker.addEventListener("duetChange", function(e) {
    console.log("selected date", e.detail.valueAsDate);
    // You can also update a Qualtrics embedded data field here if needed
    var inputTextElement = document.querySelector(".InputText");
    inputTextElement.value = e.detail.valueAsDate.toLocaleDateString();
  });
});

<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@duetds/date-picker/dist/duet/themes/default.css">
