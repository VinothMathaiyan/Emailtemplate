Qualtrics.SurveyEngine.addOnload(function() {
  jQuery('.ChoiceStructure a').on('mouseover focus', function() {
    jQuery(this).css('color', '#FFFFFF');
  });

  jQuery('.ChoiceStructure a').on('mouseout blur', function() {
    jQuery(this).css('color', '#3AE7C');
  });

  jQuery('.ChoiceStructure a').on('keydown', function(e) {
    if (e.keyCode === 32 || e.keyCode === 13) {
      jQuery(this).css('color', '#FFFFFF');
    }
  });
});
